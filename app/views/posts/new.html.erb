<%=form_for(@post) do |f| %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
  <p> <%= f.label :images %>
    <%= f.file_field :files, multiple: true  %>
  </p>
  <p> <%= f.label :videos %>
    <%= f.file_field :vids, multiple: true  %>
  </p>
  <p>
    <%= f.label :content %><br>
    <%= f.text_area :content %>
  </p>

  <p>
    <%= f.submit %>
  </p>
  
<% end %>